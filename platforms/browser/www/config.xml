<?xml version='1.0' encoding='utf-8'?>
<widget id="com.EchoSierraStudio.Peilingen" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Peilingen</name>
    <description>
    </description>
    <author email="contact@esstudio.site" href="https://esstudio.site/">
        Sem Postma
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http:*" />
    <allow-intent href="https:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarBackgroundColor" value="#f22f0d" />
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="SplashScreen" value="images/logo.png" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="500" />
    <preference name="SplashScreenBackgroundColor" value="#ffffff" />
    <preference name="SplashScreenWidth" value="256" />
    <preference name="SplashScreenHeight" value="256" />
</widget>
